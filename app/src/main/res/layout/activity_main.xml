<RelativeLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity"
    android:background="#14769A"
    android:id="@+id/principal"
    >

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/button"
        android:layout_marginTop="149dp"
        android:layout_below="@+id/imageView"
        android:layout_centerHorizontal="true" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/button"
        android:layout_toLeftOf="@+id/button"
        android:layout_toStartOf="@+id/button"
        android:background="#E0E0E0"
        android:divider="?android:dividerHorizontal"
        android:showDividers="middle"
        >

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="GRANDE BH"
            android:id="@+id/txvCidades"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:textColor="#14769A"
            android:typeface="serif"
            android:textColorHint="#f3af0f"
            android:textSize="25px"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:gravity="center"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Barreiro"
            android:id="@+id/txvBarreiro"
            android:layout_below="@+id/txvCidades"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:textColor="#000000"
            android:typeface="serif"
            android:textColorHint="#f3af0f"
            android:textSize="20px"
            android:paddingBottom="10dp"
            android:gravity="center"
            android:onClick="onClickBarreiro"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Betim"
            android:id="@+id/txvBetim"
            android:textColor="#000000"
            android:typeface="serif"
            android:textColorHint="#f3af0f"
            android:textSize="20px"
            android:paddingBottom="10dp"
            android:gravity="center"
            android:layout_below="@+id/txvBarreiro"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Contagem"
            android:id="@+id/txvContagem"
            android:textColor="#000000"
            android:typeface="serif"
            android:textColorHint="#f3af0f"
            android:textSize="20px"
            android:paddingBottom="10dp"
            android:gravity="center"
            android:layout_below="@+id/txvBetim"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Coração Eucaristico"
            android:id="@+id/txvCoreo"
            android:textColor="#000000"
            android:typeface="serif"
            android:textColorHint="#f3af0f"
            android:textSize="20px"
            android:paddingBottom="10dp"
            android:gravity="center"
            android:layout_below="@+id/txvContagem"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="São Gabriel"
            android:id="@+id/txvSG"
            android:textColor="#000000"
            android:typeface="serif"
            android:textColorHint="#f3af0f"
            android:textSize="20px"
            android:gravity="center"
            android:layout_below="@+id/txvCoreo"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="INTERIOR"
            android:id="@+id/textView"
            android:textColor="#14769A"
            android:typeface="serif"
            android:textColorHint="#f3af0f"
            android:textSize="25px"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:gravity="center"
            android:layout_below="@+id/txvSG"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Arcos"
            android:id="@+id/textView2"
            android:textColor="#000000"
            android:typeface="serif"
            android:textColorHint="#f3af0f"
            android:textSize="20px"
            android:paddingBottom="10dp"
            android:gravity="center"
            android:layout_below="@+id/textView"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Guanhães"
            android:id="@+id/textView3"
            android:textColor="#000000"
            android:typeface="serif"
            android:textColorHint="#f3af0f"
            android:textSize="20px"
            android:paddingBottom="10dp"
            android:gravity="center"
            android:layout_below="@+id/textView2"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Poços de Caldas"
            android:id="@+id/textView4"
            android:textColor="#000000"
            android:typeface="serif"
            android:textColorHint="#f3af0f"
            android:textSize="20px"
            android:paddingBottom="10dp"
            android:gravity="center"
            android:layout_below="@+id/textView3"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Serra"
            android:id="@+id/txvSerra"
            android:textColor="#000000"
            android:typeface="serif"
            android:textColorHint="#f3af0f"
            android:textSize="20px"
            android:paddingBottom="10dp"
            android:gravity="center"
            android:layout_below="@+id/textView4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/button"
        android:layout_alignLeft="@+id/button"
        android:layout_alignStart="@+id/button"
        android:id="@+id/rltLayoutOfFragment"
        android:paddingLeft="5dp"
        >


        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="140dp"
            android:id="@+id/linearLayout"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">

            <fragment
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:name="com.example.kaio.trabalho02.FragmentOne"
                android:id="@+id/fragment"
                android:layout_gravity="center_horizontal" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="140dp"
            android:layout_below="@+id/linearLayout"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">

            <fragment
                android:layout_width="match_parent"
                android:layout_height="151dp"
                android:name="com.example.kaio.trabalho02.FragmentTwo"
                android:id="@+id/fragment2"
                android:layout_gravity="right" />
        </LinearLayout>

    </RelativeLayout>

</RelativeLayout>
